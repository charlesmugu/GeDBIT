<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
    "http://struts.apache.org/dtds/struts-2.0.dtd">  
      
<struts>  
 	<package name="package" extends="json-default">
     
		<action  name="dna"  class="GeDBIT.web.DNAAction" >  
			<result  name="success">/dna.jsp</result>
		</action>
        
		<action  name="protein"  class="GeDBIT.web.ProteinAction" >  
			<result  name="success">/protein.jsp</result>
		</action>
        
		<action  name="vector"  class="GeDBIT.web.VectorAction" >  
			<result  name="success">/vector.jsp</result>
		</action>
		
		<action name="buildindex" class="GeDBIT.web.BuildIndex">
			<result name="success">/message.jsp</result>
		</action>
    </package> 
    



</struts>  